#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/web projects/JeddahFlatbed-2/JeddahFlatbed-2/node_modules/.pnpm/astro@5.13.5_@types+node@24_30b2ab877bd789fbc9deb3cfa1e6e471/node_modules/astro/node_modules:/mnt/d/web projects/JeddahFlatbed-2/JeddahFlatbed-2/node_modules/.pnpm/astro@5.13.5_@types+node@24_30b2ab877bd789fbc9deb3cfa1e6e471/node_modules:/mnt/d/web projects/JeddahFlatbed-2/JeddahFlatbed-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/web projects/JeddahFlatbed-2/JeddahFlatbed-2/node_modules/.pnpm/astro@5.13.5_@types+node@24_30b2ab877bd789fbc9deb3cfa1e6e471/node_modules/astro/node_modules:/mnt/d/web projects/JeddahFlatbed-2/JeddahFlatbed-2/node_modules/.pnpm/astro@5.13.5_@types+node@24_30b2ab877bd789fbc9deb3cfa1e6e471/node_modules:/mnt/d/web projects/JeddahFlatbed-2/JeddahFlatbed-2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
